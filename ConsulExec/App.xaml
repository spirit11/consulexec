<Application x:Class="ConsulExec.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:v="clr-namespace:ConsulExec.View"
             xmlns:vm="clr-namespace:ConsulExec.ViewModel">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:CommandStartupViewModel}">
            <v:CommandStartupView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:ProfileEditorViewModel}">
            <v:ProfileEditorView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:NodeRunViewModel}">
            <v:NodeRunView/>
        </DataTemplate>
        <!--<DataTemplate DataType="{x:Type vm:CommandRunViewModel}">
            <v:CommandRunView/>
        </DataTemplate>-->
    </Application.Resources>
</Application>
